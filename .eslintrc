{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
		"linebreak-style": 0,
    "semi": 0,
    "no-console": 0,
    "prefer-const": 0,
    "no-alert": 0,
    "no-use-before-define": 0
  }
}
